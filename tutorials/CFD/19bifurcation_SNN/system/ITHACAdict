FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
NmodesUout 5;
NmodesUproj 5;
NmodesPout 6;
NmodesPproj 6;
NmodesSUPout 6;
NmodesSUPproj 6;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;

// Specific problem of a stationary case
tolerance 1e-5;

// Export Reduced Matrices
exportPython 1;
exportMatlab 1;
exportTxt 1;

bcMethod lift;

POD_U L2;
POD_p L2;
POD_Usup L2;

normalizationNorm L2;


maxIter 200000;
